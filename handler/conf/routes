# Routes
# This file defines all application routes (Higer priority routes first)
# ~~~~

GET / controllers.IndexController.index()

GET /list controllers.IndexController.list()

GET /failures controllers.FailuresController.list()

GET /drones controllers.DronesController.list()

GET /drones/:id controllers.DronesController.show(id: Long)

GET /update controllers.IndexController.update()

POST /msg controllers.IndexController.msg()
